

<div class="container">
	<%@items.each do |image|%>
	<div class="box">
		<div class="imgBox">
				<%=link_to item_path(image.id) do%>
				<%=image_tag image.image_url%>
				<%end%>
		</div>
			<div class="details">
				<div class="containt">
					<%=link_to item_path(image.id) do%>
				<h2><%= image.title %><h2>
						<%end%>
				</div>
			</div>
			

	</div>

			<% end %>
</div>
